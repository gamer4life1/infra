version: "3.6"
services:
  bitwarden:
    container_name: bitwarden
    environment:
      - WEBSOCKET_ENABLED=true
    image: vaultwarden/server:latest
    restart: unless-stopped
    ports:
      - 8695:80
      - 3012:3012
    volumes:
        - /opt/docker/data/bitwarden:/data

